<!DOCTYPE html>
<html lang="sv-se">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @if (!string.IsNullOrWhiteSpace(ViewData["Title"]?.ToString()))
    {
        <Title>@ViewData["Title"] - Silicon</Title>
    }
    else
    {
        <title> Silicon</title>
    }
    <script src="https://kit.fontawesome.com/41c8b474e2.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    <link rel="icon" type="image/svg" sizes="16x16" href="~/Images/icons/silicon-icon-light-theme.svg">
    <title>@ViewData["Title"] - Silicon</title>
</head>
<body>


    <div class="wrapper">
        @Html.Partial("~/Views/Sections/_HeaderPartial.cshtml")
       

        <main>
            @RenderBody()
        </main>

        @Html.Partial("~/Views/Sections/_FooterPartial.cshtml")

    </div>


    <script src="~/js/site.js" asp-append-version="true"></script>

</body>
</html>